{
  "name": "wac-ldp-kit",
  "version": "0.1.0",
  "description": "A resource store for use in LDP-like servers such as Solid servers. Also usable as LDP server.",
  "main": "dist/app.js",
  "dependencies": {
    "@types/node": "^11.12.1",
    "debug": "^4.1.1",
    "stream": "0.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "mocha": "^6.1.2",
    "sinon": "^7.3.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.2"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "DEBUG=* ts-node-dev --project \"tsconfig.json\" src/server.ts",
    "build": "tsc -p tsconfig.json && npm run docs",
    "mocha": "mocha --require ts-node/register test/**/*.ts",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "npm run lint && npm run mocha",
    "docs": "typedoc --out docs --mode modules src",
    "testsuite1": "java -jar ../../w3c/ldp-testsuite/target/ldp-testsuite-0.2.0-SNAPSHOT-shaded.jar --basic --test testJsonLdRepresentation",
    "testsuite": "java -jar ../../w3c/ldp-testsuite/target/ldp-testsuite-0.2.0-SNAPSHOT-shaded.jar --basic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michielbdejong/ldp-resource-store.git"
  },
  "author": "inrupt, Inc.",
  "contributors": [
    "Michiel de Jong (https://github.com/michielbdejong)",
    "Ruben Verborgh (https://github.com/rubenverborgh)",
    "Kjetil Kjernsmo (https://github.com/kjetilk)",
    "*Jackson Morgan (https://github.com/jaxoncreed)",
    "Pat McBennett (https://github.com/pmcb55)",
    "Justin Bingham (https://github.com/justinwb)"
  ],
